        "format": "run-s format:*",
        "format:eslint": "eslint --ext .js,.html includes/layouts/ --fix",
        "validate": "run-s build lint"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-img": "^0.9.0",
