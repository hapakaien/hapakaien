        "@snowpack/plugin-run-script": "^2.3.0",
        "@tailwindcss/typography": "^0.4.0",
        "autoprefixer": "^10.2.5",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.2",
        "eleventy-plugin-svg-contents": "^0.7.0",
        "eslint": "^7.26.0",
